{
  "name": "CHMM[efe]",
  "module": "agents.impl.CHMM",
  "class": "CHMM",
  "encoder": {
    "module": "agents.networks.EncoderNetworks",
    "class": "Conv64"
  },
  "decoder": {
    "module": "agents.networks.DecoderNetworks",
    "class": "Conv64"
  },
  "transition": {
    "module": "agents.networks.TransitionNetworks",
    "class": "LinearRelu3x100"
  },
  "critic": {
    "module": "agents.networks.CriticNetworks",
    "class": "LinearRelu4x100"
  },
  "strategy": {
    "module": "agents.strategies.EpsilonGreedy",
    "class": "EpsilonGreedy",
    "epsilon_start": "0.9",
    "epsilon_end": "0.05",
    "epsilon_decay": "1000"
  },
  "discount_factor": "0.9",
  "n_steps_between_synchro": "10",
  "queue_capacity": "50000",
  "beta": "1.0",
  "vfe_lr": "0.0001",
  "critic_lr": "0.0001",
  "n_states": "10",
  "critic_objective": "Expected Free Energy"
}