{
  "name": "HMM",
  "module": "agents.impl.HMM",
  "class": "HMM",
  "encoder": {
    "module": "agents.networks.EncoderNetworks",
    "class": "Conv64"
  },
  "decoder": {
    "module": "agents.networks.DecoderNetworks",
    "class": "Conv64"
  },
  "transition": {
    "module": "agents.networks.TransitionNetworks",
    "class": "LinearRelu3x100"
  },
  "strategy": {
    "module": "agents.strategies.RandomActions",
    "class": "RandomActions"
  },
  "queue_capacity": "50000",
  "beta": "1.0",
  "vfe_lr": "0.0001",
  "n_states": "10"
}