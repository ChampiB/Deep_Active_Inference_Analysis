{
  "name": "CHMM_AGENT",
  "module": "agents.impl.CHMM",
  "class": "CHMM",
  "encoder": {
    "module": "agents.networks.EncoderNetworks",
    "class": "ConvDAIMC"
  },
  "decoder": {
    "module": "agents.networks.DecoderNetworks",
    "class": "ConvDAIMC"
  },
  "transition": {
    "module": "agents.networks.TransitionNetworks",
    "class": "LinearReluDropout4x512"
  },
  "critic": {
    "module": "agents.networks.CriticNetworks",
    "class": "LinearReluDropout4x100"
  },
  "strategy": {
    "module": "agents.strategies.EpsilonGreedy",
    "class": "EpsilonGreedy",
    "epsilon_start": "1",
    "epsilon_end": "0",
    "epsilon_decay": "200"
  },
  "discount_factor": "0.8",
  "n_steps_between_synchro": "15",
  "queue_capacity": "10000",
  "beta": "0.2",
  "vfe_lr": "0.1",
  "critic_lr": "0.1",
  "n_states": "13",
  "critic_objective": "Expected Free Energy"
}