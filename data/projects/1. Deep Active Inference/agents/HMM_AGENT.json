{
  "name": "HMM_AGENT",
  "module": "agents.impl.HMM",
  "class": "HMM",
  "encoder": {
    "module": "agents.networks.EncoderNetworks",
    "class": "ConvDAIMC"
  },
  "decoder": {
    "module": "agents.networks.DecoderNetworks",
    "class": "ConvDAIMC"
  },
  "transition": {
    "module": "agents.networks.TransitionNetworks",
    "class": "LinearReluDropout4x512"
  },
  "strategy": {
    "module": "agents.strategies.SoftmaxSampling",
    "class": "SoftmaxSampling",
    "gain": "3"
  },
  "queue_capacity": "1000",
  "beta": "0.3",
  "vfe_lr": "0.1",
  "n_states": "15"
}